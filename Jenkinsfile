pipeline {
    agent any
    stages {
      stage ('Build') {
        steps {
        echo "Build Step"}
          sleep 5
      
        stage ('test') {
        steps {
        echo "Test Step"}
            sleep 5
      
      }stage ('deploy') {
        steps {
        echo "Deploy war file"}
      
      }stage ('hello') {
        steps {
        echo "Hello World"}
            sleep 5
        }
      }
    }

}
